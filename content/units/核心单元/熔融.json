    "color":"FFD37FFF",
  },
    {
      "type":"HoverPart",
      "mirror":true,
      //是否镜像显示
      "x":4,
      "y":-4,
      //起始位置
      "radius":6,
      "phase":90,
      "stroke":3,
      "layerOffset":-0.001,
      "color":"FFD37FFF",
    }]

  "weapons": [{
    "x": 0,
    // 位置 x 坐标
    "y": 3,
    // 位置 y 坐标
    "mirror":false,
    // 是否镜像
    "reload": 4,
    // 装弹速度
    "rotate": false,
    // 是否旋转

    "bullet": {
      "type": "BasicBulletType",
      // 子弹类型
      "damage": 5,
      // 伤害
      "frontColor": "FFD37FFF",
      // 正面颜色
      "backColor": "FFD37FFF",
      // 背景色
      "width": 2,
      // 宽度
      "height": 15,
      // 长度
      "speed": 5,
      // 速度
      "lifetime": 40,
