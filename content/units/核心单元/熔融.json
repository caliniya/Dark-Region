  // 是否能治疗单位
  "health": 500,
  // 生命值
  "armor": 5,
  // 装甲
  "mineSpeed": 9,
  // 挖矿速度
  "mineTier": 5,
  // 挖矿等级
  "buildSpeed": 1,
  // 构建速度
  "rotateSpeed": 40,
  // 转速
  "itemCapacity": 30,
  // 容量
  "range": 40
  "parts":[{
    "type":"HoverPart",
    "mirror":true,
    //是否镜像显示
    "x":4,
    "y":4,
    //起始位置
    "radius":6,
    //半径
    "phase":90,
    //相位
    "stroke":3,
    //粗细
    "layerOffset":-0.001,
