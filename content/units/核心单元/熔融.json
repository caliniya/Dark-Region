      // 追踪范围
      "healPercent": 10,
      // 治疗百分比
      "collidesGround": true,
      // 是否与地面碰撞
      "collidesAir": true,
      // 是否与空中单位碰撞
      "collidesTeam": true,
      // 是否与队伍碰撞
      "targetUnits": true,
      // 是否能攻击单位
      "targetBuildings": true,
      // 是否能攻击建筑
    }
  },
    {
      "type": "RepairBeamWeapon",
      //修复友军
      "x": 3,
      // 位置 x 坐标
      "y": 2,
      // 位置 y 坐标
      "mirror":true,
      // 是否镜像
      "rotate": true,
      // 是否旋转
      "repairSpeed":0.75
      //修复速度
      "bullet": {
        "type":"BulletType",
        "maxRange" :120,
      }
